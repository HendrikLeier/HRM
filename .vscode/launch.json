{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": " debug maze dataset",
            "type": "debugpy",
            "request": "launch",
            "program": "dataset/build_maze_dataset.py",
        },
        {
            "name": "Debug: Sudoku Single GPU",
            "type": "debugpy",
            "request": "launch",
            "program": "pretrain.py",
            "args": [
                "data_path=data/sudoku-extreme-1k-aug-1000",
                "epochs=20000",
                "eval_interval=2000",
                "global_batch_size=384",
                "lr=7e-5",
                "puzzle_emb_lr=7e-5",
                "weight_decay=1.0",
                "puzzle_emb_weight_decay=1.0"
            ],
            "env": {
                "OMP_NUM_THREADS": "1",
                "DISABLE_COMPILE": "true"
            }
        },
         {
            "name": "Debug: Inference",
            "type": "debugpy",
            "request": "launch",
            "program": "inference.py",
            "args": [
                "+dataset_path=data/maze-30x30-hard-1k",
                "+vocab_size=16",
                "+seq_len=21",
                "+num_puzzle_identifiers=1"
            ],
            "env": {
                "OMP_NUM_THREADS": "1",
                "DISABLE_COMPILE": "true"
            }
        },
        {
            "name": "Debug: Maze Single GPU",
            "type": "debugpy",
            "request": "launch",
            "program": "pretrain.py",
            "args": [
                "data_path=data/maze-30x30-hard-1k",
                "epochs=20000",
                "eval_interval=2000",
                "lr=1e-4",
                "puzzle_emb_lr=1e-4",
                "weight_decay=1.0",
                "puzzle_emb_weight_decay=1.0"
            ],
            "env": {
                "OMP_NUM_THREADS": "1",
                "DISABLE_COMPILE": "true"
            }
        },
        {
            "name": "Debug: Math Single GPU",
            "type": "debugpy",
            "request": "launch",
            "program": "pretrain.py",
            "args": [
                "data_path=data/math-100-simple",
                "epochs=20000",
                "eval_interval=1",
                "lr=1e-4",
                "global_batch_size=100",
                "puzzle_emb_lr=1e-4",
                "weight_decay=1.0",
                "puzzle_emb_weight_decay=1.0"
            ],
            "env": {
                "OMP_NUM_THREADS": "1",
                "DISABLE_COMPILE": "true"
            }
        }
    ]
}